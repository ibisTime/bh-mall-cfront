.address-wrapper {
  background: #fafafa;
}
.address-wrapper .item {
  margin-bottom: 20rpx;
  background: #fff;
}
.address-wrapper .item .addr-info {
  flex: 1;
}
.address-wrapper .item .addr-cont {
  display: flex;
  padding: 40rpx 30rpx;
  border-bottom: 1rpx solid #eee;
}
.address-wrapper .item .addr-cont .info {
  font-size: 30rpx;
  color: #484848;
}
.address-wrapper .item .addr-cont .name {
  padding-right: 20rpx;
}
.address-wrapper .item .addr-cont .addr {
  padding-top: 24rpx;
  font-size: 26rpx;
  color: #999;
}
.address-wrapper .item .addr-cont .chose {
  width: 40rpx;
  flex: 0 0 40rpx;
  background-size: 30rpx;
  background-position: right center;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}
.address-wrapper .item .addr-cont .chose.act {
  background-image: url('data:image/png;base64,');
}
.address-wrapper .addr-list .addr-operator {
  padding: 20rpx 30rpx;
  font-size:  28rpx;
  color: #484848;
}
.address-wrapper .addr-list .addr-operator .img {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.address-wrapper .addr-list .default {
  float: left;
  padding-left: 40rpx;
  background-size: 30rpx;
  background-position: left center;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}
.address-wrapper .addr-list .default.act {
  background-image: url('data:image/png;base64,');
}
.address-wrapper .addr-list .addr-operator .delete {
  float: right;
  margin-left: 30rpx;
}
.address-wrapper .addr-list .addr-operator .edit {
  float: right;
}
.address-wrapper .footer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #72a52c;
  color: #fff;
  text-align: center;
  font-size: 32rpx;
}