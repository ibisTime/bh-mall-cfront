.edit-address .form-wrapper .item {
  display: flex;
  min-height: 100rpx;
  padding: 0 30rpx;
  align-items: center;
  border-bottom: 1rpx solid #eee;
  font-size: 30rpx;
  color: #484848;
  background-color: #fff;
}
.edit-address .form-wrapper .item:last-child {
  border-bottom: none;
}
.edit-address .form-wrapper .picker {
  background-image: url('data:image/png;base64,');
  background-size: 20rpx;
  background-position: right center;
  background-repeat: no-repeat;
}
.edit-address .form-wrapper .item .title {
  flex: 0 0 150rpx;
}
.edit-address .form-wrapper .item .input {
  flex: 1;
  padding-left: 40rpx;
}
.edit-address .form-wrapper .item .picker {
  width: 100%;
}
.edit-address .form-wrapper .item .dtitle {
  flex: 1;
  text-align: left;
}
.edit-address .form-wrapper .item .chose {
  width: 40rpx;
  height: 100rpx;
  background-position: center;
  background-size: 40rpx;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}
.edit-address .form-wrapper .item .chose.act {
  background-image: url('data:image/png;base64,');
}
.mt20 {
  margin-top: 20rpx;
}
.edit-address .footer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #72a52c;
  color: #fff;
  text-align: center;
  font-size: 32rpx;
}